  right: 0rpx;
  top: 0rpx;
}
.wode_content_item button{
  width: 700rpx;
  height: 83rpx;
  position: absolute;
  opacity: 0;
}

.protocol-dialog {
  position: relative;
}

.protocol-dialog .mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: #00000090;
}

.protocol-dialog-wrapper {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
  display: flex;
  align-items: center;
  justify-content: center;
}

.protocol-dialog-content {
  background: #ffffff;
  width: 440rpx;
  padding: 0 30rpx;
  border-radius: 30rpx;
  box-sizing: border-box;
}

.protocol-dialog-content .top-logo {
  padding: 0 10rpx;
  position: relative;
}

.protocol-dialog-content .top-logo image {
  width: 100%;
  position: relative;
  top: -10rpx;
}

.protocol-dialog-content .top-logo .title {
  position: absolute;
  bottom: 60rpx;
  left: 0;
  right: 0;
  color: #333333;
  font-size: 38rpx;
  font-weight: bold;
  text-align: center;
}

.protocol-dialog-content .protocol-tip {
  color: #333333;
  font-size: 24rpx;
  line-height: 36rpx;
  text-indent: 50rpx;
  text-align: justify;
  position: relative;
  top: -40rpx;
}

.protocol-dialog-content .protocol-tip navigator {
  color: #F25B5C;
  display: inline;
}

.protocol-dialog-content .options {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 30rpx
}

.protocol-dialog-content .options view {
  width: 185rpx;
  height: 55rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: 26rpx;
  border-radius: 28rpx;
}

.protocol-dialog-content .options view:nth-child(1) {
  background: #E6E6E6;
  color: #999999;
}

.protocol-dialog-content .options view:nth-child(2) {
  background: #F25B5C;
  color: #FFFFFF;
}

.icon_vip-wrap {
  position: relative;
  display: flex;
  align-items: center;
}

.pt-vip-text {
  position: absolute;
  left: 0;
  right: 0;
  color: #FFE2B2;
  font-size: 20rpx;
  text-align: center;
  /* top: 2rpx; */
}

.vip-text {
  position: absolute;
  left: 0;
  right: 0;
  font-size: 20rpx;
  text-align: center;
  /* top: -2rpx; */
}

.renew {
  margin-right: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.renew image {
  width: 181rpx;
  margin-bottom: 14rpx;
}

.renew text {
  font-size: 18rpx;
  color: #F5CEC3;
}

.icon_vip {
  width: 100rpx;
  position: relative;
}