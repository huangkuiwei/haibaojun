  left: 25rpx;
}
.wode_content_item_icon{
  width: 35rpx;
  height: 35rpx;
  margin-right: 23rpx;
}
.wode_content_item_name{
  height: 83rpx;
  line-height: 83rpx;
  font-size:28rpx;
  color:rgba(51,51,51,1);
}
.wode_content_item_arrow{
  height: 83rpx;
  line-height: 83rpx;
  font-size:28rpx;
